    <!-- Modal Universal untuk Fitur Gemini -->
    <div id="geminiModal" class="fixed inset-0 bg-gray-800 bg-opacity-75 overflow-y-auto h-full w-full flex items-center justify-center hidden z-[100]">
        <div class="relative mx-auto p-6 border w-full max-w-lg shadow-xl rounded-xl bg-white">
            <div class="flex justify-between items-center mb-4">
                <h3 id="geminiModalTitle" class="text-xl leading-6 font-bold text-slate-900">Judul Modal</h3>
                <button id="geminiModalCloseBtn" class="text-slate-400 hover:text-slate-600">
                    <span class="text-2xl">&times;</span>
                </button>
            </div>
            <div id="geminiModalBody" class="text-sm text-slate-700">
                <!-- Konten modal (input atau hasil) akan dimasukkan di sini -->
            </div>
            <div id="geminiModalLoading" class="loader hidden my-4"></div>
            <div id="geminiModalActions" class="mt-6 flex justify-end space-x-3">
                <!-- Tombol aksi modal akan dimasukkan di sini -->
            </div>
        </div>
    </div>


    <script>
        const contentData = [
            { type: 'positif', category: 'Edukasi Budaya & Kebhinekaan', examples: 'Unggahan tentang tari tradisional, festival budaya lokal, kuliner khas daerah, dialog antarbudaya, dan promosi pakaian adat.', impact: 'Meningkatkan apresiasi terhadap keberagaman budaya dan memperkuat identitas nasional.', icon: '🎉' },
            { type: 'positif', category: 'Aksi Sosial & Gotong Royong', examples: 'Dokumentasi bakti sosial mahasiswa, penggalangan dana, ajakan kolaborasi proyek komunitas, dan kampanye peduli lingkungan.', impact: 'Mendorong partisipasi, memperkuat ikatan sosial, menumbuhkan empati, dan mempraktikkan gotong royong.', icon: '🤝' },
            { type: 'positif', category: 'Kampanye Persatuan Positif', examples: 'Konten dengan tagar #IndonesiaBersatu, unggahan tokoh inspiratif, kutipan pahlawan, dan video pendek tentang makna persatuan.', impact: 'Meningkatkan kesadaran akan pentingnya persatuan dan menginspirasi tindakan positif untuk menjaga keutuhan bangsa.', icon: '🇮🇩' },
            { type: 'negatif', category: 'Hoaks & Disinformasi', examples: 'Berita bohong yang menyudutkan kelompok SARA, narasi politik memecah belah, dan teori konspirasi.', impact: 'Menimbulkan kecurigaan, ketegangan sosial, perpecahan, dan mengikis kepercayaan pada institusi.', icon: '🔥' },
            { type: 'negatif', category: 'Ujaran Kebencian & Provokasi', examples: 'Komentar atau unggahan yang merendahkan berdasarkan SARA, pandangan politik, atau identitas lainnya.', impact: 'Memicu kemarahan, konflik, merusak iklim dialog, dan menciptakan lingkungan digital yang toksik.', icon: '😡' },
            { type: 'negatif', category: 'Polarisasi & Echo Chamber', examples: 'Konten yang eksklusif mendukung satu pandangan dan menyerang pandangan lain, diperkuat oleh algoritma.', impact: 'Mengurangi toleransi, menyulitkan dialog konstruktif, dan memperdalam jurang pemisah dalam masyarakat.', icon: '🌀' }
        ];

        const contentCardsContainer = document.getElementById('content-cards');
        const showAllBtn = document.getElementById('showAllBtn');
        
        const geminiModal = document.getElementById('geminiModal');
        const geminiModalTitle = document.getElementById('geminiModalTitle');
        const geminiModalBody = document.getElementById('geminiModalBody');
        const geminiModalActions = document.getElementById('geminiModalActions');
        const geminiModalCloseBtn = document.getElementById('geminiModalCloseBtn');
        const geminiModalLoading = document.getElementById('geminiModalLoading');

        const analyzeTextBtn = document.getElementById('analyzeTextBtn');
        const generateContentIdeaBtn = document.getElementById('generateContentIdeaBtn');

        function createCard(item) {
            return `
                <div class="card bg-white rounded-lg shadow-md border border-slate-200 overflow-hidden flex flex-col">
                    <div class="p-6">
                        <div class="flex items-center">
                            <div class="flex-shrink-0 text-3xl mr-4">${item.icon}</div>
                            <div><h4 class="font-bold text-xl ${item.type === 'positif' ? 'text-green-600' : 'text-red-600'}">${item.category}</h4></div>
                        </div>
                        <div class="mt-4"><p class="text-sm font-semibold text-slate-700">Contoh Konten:</p><p class="text-sm text-slate-600">${item.examples}</p></div>
                        <div class="mt-3"><p class="text-sm font-semibold text-slate-700">Potensi Dampak:</p><p class="text-sm text-slate-600">${item.impact}</p></div>
                    </div>
                </div>`;
        }

        function renderCards(filterType = null) {
            contentCardsContainer.innerHTML = '';
            const filteredData = filterType ? contentData.filter(item => item.type === filterType) : contentData;
            filteredData.forEach(item => { contentCardsContainer.innerHTML += createCard(item); });
        }
        
        async function callGeminiAPI(prompt) {
            geminiModalLoading.classList.remove('hidden');
            geminiModalBody.innerHTML = ''; // Clear previous results or input form

            let chatHistory = [{ role: "user", parts: [{ text: prompt }] }];
            const payload = { contents: chatHistory };
            const apiKey = ""; // API Key akan di-inject oleh environment jika kosong
            const apiUrl = `https://generativelanguage.googleapis.com/v1beta/models/gemini-2.0-flash:generateContent?key=${apiKey}`;

            try {
                const response = await fetch(apiUrl, {
                    method: 'POST',
                    headers: { 'Content-Type': 'application/json' },
                    body: JSON.stringify(payload)
                });

                if (!response.ok) {
                    const errorBody = await response.text();
                    console.error("API Error Response:", errorBody);
                    throw new Error(`API request failed with status ${response.status}: ${response.statusText}`);
                }
                
                const result = await response.json();

                if (result.candidates && result.candidates.length > 0 &&
                    result.candidates[0].content && result.candidates[0].content.parts &&
                    result.candidates[0].content.parts.length > 0) {
                    return result.candidates[0].content.parts[0].text;
                } else {
                    console.error("Unexpected API response structure:", result);
                    let feedbackMessage = "Gagal mendapatkan respons dari AI atau format respons tidak sesuai.";
                    if (result.promptFeedback) {
                        console.error("Prompt Feedback:", result.promptFeedback);
                        feedbackMessage = "Gagal mendapatkan respons dari AI karena masalah dengan prompt.";
                        if (result.promptFeedback.blockReason) {
                            feedbackMessage += ` Alasan: ${result.promptFeedback.blockReason}.`;
                        }
                        if (result.promptFeedback.safetyRatings && result.promptFeedback.safetyRatings.length > 0) {
                            feedbackMessage += " Detail Peringkat Keamanan: ";
                            result.promptFeedback.safetyRatings.forEach(rating => {
                                feedbackMessage += `${rating.category} - ${rating.probability}; `;
                            });
                        }
                    }
                    return feedbackMessage;
                }
            } catch (error) {
                console.error('Error calling Gemini API:', error);
                return `Terjadi kesalahan saat menghubungi AI: ${error.message}. Silakan coba lagi nanti.`;
            } finally {
                geminiModalLoading.classList.add('hidden');
            }
        }

        function showGeminiModal(title, bodyContent, actionsContent) {
            geminiModalTitle.textContent = title;
            geminiModalBody.innerHTML = bodyContent;
            geminiModalActions.innerHTML = actionsContent;
            geminiModal.classList.remove('hidden');
        }

        geminiModalCloseBtn.addEventListener('click', () => geminiModal.classList.add('hidden'));

        analyzeTextBtn.addEventListener('click', () => {
            const modalBody = `
                <p class="mb-2 text-sm">Masukkan teks unggahan Instagram yang ingin Anda analisis:</p>
                <textarea id="textToAnalyze" class="w-full h-32 p-2 border border-slate-300 rounded-md focus:ring-amber-500 focus:border-amber-500" placeholder="Contoh: Mari kita jaga persatuan bangsa! #IndonesiaDamai"></textarea>
                <div id="analysisResult" class="mt-4 p-3 bg-slate-50 rounded-md border border-slate-200 modal-content-display"></div>`;
            const modalActions = `<button id="submitAnalysisBtn" class="bg-amber-500 text-white px-4 py-2 rounded-md hover:bg-amber-600">Analisis Teks</button>`;
            showGeminiModal('✨ Analisis Potensi Dampak Teks', modalBody, modalActions);

            document.getElementById('submitAnalysisBtn').addEventListener('click', async () => {
                const text = document.getElementById('textToAnalyze').value;
                const resultContainer = document.getElementById('analysisResult');
                if (!text.trim()) {
                    resultContainer.textContent = "Silakan masukkan teks untuk dianalisis.";
                    return;
                }
                resultContainer.textContent = ''; // Clear previous result
                const prompt = `Anda adalah seorang analis media sosial yang ahli dalam Pancasila. Analisis teks berikut seolah-olah ini adalah unggahan di Instagram. Fokus pada sentimennya (positif, negatif, atau netral) dan potensi dampaknya terhadap nilai persatuan Indonesia (Sila Ketiga Pancasila) di kalangan mahasiswa. Berikan analisis Anda dalam poin-poin singkat dan jelas. Teks: "${text}"`;
                const analysis = await callGeminiAPI(prompt);
                resultContainer.textContent = analysis;
            });
        });

        generateContentIdeaBtn.addEventListener('click', async () => {
            showGeminiModal('✨ Ide Konten Persatuan', '<p>Sedang memuat ide konten...</p>', '');
            const prompt = "Anda adalah seorang kreator konten yang ahli dalam mempromosikan nilai-nilai Pancasila. Berikan 3 ide unggahan Instagram yang kreatif dan menarik untuk mahasiswa, yang bertujuan memperkuat persatuan Indonesia (Sila Ketiga Pancasila). Setiap ide harus mencakup: (1) Konsep singkat, (2) Saran visual (misalnya, jenis gambar/video), dan (3) Contoh caption singkat dengan tagar yang relevan. Format sebagai daftar bernomor dengan sub-poin yang jelas.";
            const ideas = await callGeminiAPI(prompt);
            geminiModalBody.innerHTML = `<div class="modal-content-display p-3 bg-slate-50 rounded-md border border-slate-200">${ideas}</div>`;
        });


        document.addEventListener('DOMContentLoaded', () => {
            const ctx = document.getElementById('impactChart').getContext('2d');
            const impactChart = new Chart(ctx, {
                type: 'doughnut',
                data: {
                    labels: ['Potensi Positif', 'Potensi Negatif'],
                    datasets: [{
                        label: 'Dampak Instagram', data: [50, 50],
                        backgroundColor: ['rgba(34, 197, 94, 0.7)', 'rgba(239, 68, 68, 0.7)'],
                        borderColor: ['rgba(34, 197, 94, 1)', 'rgba(239, 68, 68, 1)'],
                        borderWidth: 2, hoverOffset: 15,
                    }]
                },
                options: {
                    responsive: true, maintainAspectRatio: false, cutout: '50%',
                    plugins: {
                        legend: { position: 'bottom', labels: { padding: 20, font: { size: 14, weight: '500' }, color: '#334155' }},
                        tooltip: { callbacks: { label: function(context) { return ` Klik untuk melihat konten ${context.label}`; } } }
                    },
                    onClick: (event, elements) => {
                        if (elements.length > 0) {
                            const i = elements[0].index;
                            renderCards(i === 0 ? 'positif' : 'negatif');
                        }
                    }
                }
            });

            showAllBtn.addEventListener('click', () => renderCards());
            renderCards(); 
        });

        document.querySelectorAll('a[href^="#"]').forEach(anchor => {
            anchor.addEventListener('click', function (e) {
                e.preventDefault();
                document.querySelector(this.getAttribute('href')).scrollIntoView({ behavior: 'smooth' });
            });
        });
    </script>

</body>
</html>
